<?xml version="1.0" encoding="UTF-8"?>
<!-- This file contains common properties used by the build-in2out transformation -->
<Properties>
    <!-- The name of the survey, conditionning the name of the input file processed. -->
    <SurveyName>esa-2021-a00</SurveyName>
    
    <!-- The in-format to be read : ddi / pogues-xml -->
    <InFormat>ddi</InFormat>
    
    <!-- The out-format to be written : xforms / fo / fodt / ddi / lunatic-xml / pogues-xml -->
    <OutFormat>xforms</OutFormat>
    
    <!-- (re)Build Eno before generate true / false -->
    <EnoBuild>true</EnoBuild>
    
    <!-- debug mode : true / false -->
    <Debug>false</Debug>

</Properties>